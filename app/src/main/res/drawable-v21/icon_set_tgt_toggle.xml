<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item
        android:id="@+id/icon_set_tgt_off_state"
        android:state_checked="false"
        android:drawable="@drawable/icon_set_tgt_off"/>
    <item
        android:id="@+id/icon_set_tgt_on_state"
        android:state_checked="true"
        android:drawable="@drawable/icon_set_tgt_on"/>

    <transition
        android:drawable="@drawable/icon_set_tgt_toggle_on"
        android:fromId="@id/icon_set_tgt_off_state"
        android:toId="@id/icon_set_tgt_on_state" />

    <transition
        android:drawable="@drawable/icon_set_tgt_toggle_off"
        android:fromId="@id/icon_set_tgt_on_state"
        android:toId="@id/icon_set_tgt_off_state" />
</animated-selector>